menu "GPIO Utilities"

INSERT

config BUTTON
	bool "button"
	default y
	select ROUTE
	help
	  Enable button support
	  Button sends a network packet whenever pressed (requires GPIO).


config BUTTONDSR
        bool "buttondsr"
        default y
        select ROUTE
        help
          Button runs a program whenever dsr is shorted to ground.


endmenu
